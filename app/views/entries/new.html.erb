<%= form_for [@project, @entry] do |f|%>
	<%= f.label :hours %>
	<%= f.text_field :hours %>	
	<br>
	<br>

	<%= f.label :minutes %>
	<%= f.text_field :minutes %>	
	<br>
	<br>

	<%= f.label :date %>
	<%= f.text_field :date, :placeholder => "dd/mm/yyyy" %>	
	<br>
	<br>

	<%= f.label :comments %>
	<%= f.text_field :comments %>	
	<br>
	<br>
	<%= f.submit "Create entry" %>

<% end %>